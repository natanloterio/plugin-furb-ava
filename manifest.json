{
  "manifest_version": 2,
 "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["replace.js"],
	  "run_at": "document_end"
    }
  ],
 
  "name": "PluginFurb",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "2.9",
  "browser_action": {
    "default_icon": "icon.png"
    ,"default_popup": "popup.html"
  },	
	"permissions": [
		"tabs", "http://*/*", "https://*/*","http://*.furb.br/"
	]
}